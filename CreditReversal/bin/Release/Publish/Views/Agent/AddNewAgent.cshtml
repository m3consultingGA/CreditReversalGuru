@model CreditReversal.Models.Agent
@{
    if (Session["UserRole"] != null && Session["UserName"] != null)
    {
        if (Request.QueryString["Mode"] == "Edit")
        {
            ViewBag.Title = "Edit Agent";
        }
        else
        {
            ViewBag.Title = "Create Agent";
        }

    }
    else
    {
        ViewBag.Title = "Agent Registration";
    }
    if (Session["UserRole"] != null && Session["UserName"] != null)
    {
        Layout = "~/Views/Shared/_secureLayoutPage.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_publicLayoutPage.cshtml";
    }

}
<br />
<link href="~/Content/css/jquery.steps.css" rel="stylesheet" type="text/css" />
<div class="page-header" style="padding-bottom:20px;">
    <ol class="breadcrumb pull-right">
        <li class="breadcrumb-item"><a href="@ViewBag.Dasboard"><i class="fa fa-dashboard"></i>&nbsp;Dashboard</a></li>
        <li class="breadcrumb-item"><a href="/Agent/Index"><i class="fa fa-user"></i>&nbsp;Manage Agents</a></li>
        @{
            if (Request != null)
            {
                if (Request.QueryString["Mode"] == "Edit")
                {
                    <li class="breadcrumb-item active">Edit Agent </li>
                }
                else
                {
                    <li class="breadcrumb-item active">Create Agent </li>
                }
            }
        }

    </ol>
</div>

<div class="portlet portlet-boxed">
    <div class="portlet-header">

        @if (Session["UserRole"] != null && Session["UserName"] != null)
        {
            if (Request.QueryString["Mode"] == "Edit")
            {
                <h4 class="portlet-title">Edit Agent</h4>
            }
            else
            {
                <h4 class="portlet-title">Create Agent</h4>
            }

        }


    </div> <!-- /.portlet-header -->
    <div class="portlet-body" style="padding-top:10px;">

        <ul class="nav nav-tabs" id="guide-tabs">
            <li class="Business active"><a data-toggle="tab" href="#home" id="navBusiness"><h6>Business Information</h6></a></li>
            <li class="Address"><a data-toggle="tab" href="#Address" id="navAddress"><h6>Address</h6></a></li>
            <li class="Billing"><a data-toggle="tab" href="#Billing" id="navBilling"><h6>Billing Information</h6></a></li>
            <li class="User"><a data-toggle="tab" href="#User" id="navUser"><h6>Primary User</h6></a></li>

        </ul>
        <div class="loader"> </div>
        <div id="overlay"></div>
        <form method="post" action="/Agent/AddNewAgent" id="example-advanced-form"  enctype="multipart/form-data" class="wizard-enhanced wizard-horizontal parsley-form">
            <div class="tab-content">
                <div id="home" class="tab-pane fade in active">
                    <div class="container">
                        <div class="col-lg-12">
                            <div class="form-group" id="divBusinessName">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Business Name <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" name="BusinessName" id="BusinessName" placeholder="Business Name" onchange="RemoveValidation(this);" class="form-control" required />
                                        @if (Request != null)
                                        {
                                            if (Request.QueryString["Id"] != null)
                                            {

                                                <input type="hidden" name="AgentId" id="AgentId" value="@Request.QueryString["Id"]" />
                                            }

                                        }

                                        <div id="Msg_BusinessName" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>



                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Type Of Company<span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <select class="form-control" id="TypeOfComp" name="TypeOfComp" onchange="RemoveValidation(this);" required>
                                            <option value="0">Select Company Type</option>
                                            @if (ViewBag.companytypes != null)
                                            {
                                                foreach (var item in ViewBag.companytypes)
                                                {
                                                    <option value="@item.CompanyTypeId">@item.CompanyType</option>
                                                }
                                            }
                                        </select>
                                        <div id="Msg_TypeOfComp" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="form-group" id="divDBA">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="DBA">DBA </label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control" id="DBA" name="DBA" placeholder="DBA">
                                        <div id="Msg_DBA" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divtxtSSN">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Social Security Number <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="SocialSecCardNo" name="SocialSecCardNo" class="form-control" maxlength="50" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_SocialSecCardNo" class="text-danger">
                                        </div>
                                    </div>

                                </div>
                            </div>


                            <div class="form-group" id="divFileSocialSecCard">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Social Security Card <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="file" id="SocialSecCard" name="SocialSecCard" class="form-control" accept=".pdf,.jpeg,.png,.jpg" onchange="RemoveValidation(this);" required />
                                        <label id="lblSocialSecCard"></label>
                                        <div id="Msg_SocialSecCard" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="form-group" id="divDriversLicenseNumber">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Drivers License Number <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="DriversLicenseNumber" name="DriversLicenseNumber" class="form-control" maxlength="50" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_DriversLicenseNumber" class="text-danger">
                                        </div>
                                    </div>

                                </div>
                            </div>

                            <div class="form-group" id="divDriversLicenseState">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Drivers License State<span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <select class="form-control" id="DriversLicenseState" name="DriversLicenseState" onchange="RemoveValidation(this);" required>
                                            <option value="0">Select Drivers License State</option>
                                            @if (ViewBag.States != null)
                                            {
                                                foreach (var item in ViewBag.States)
                                                {
                                                    <option value="@item.StateCode">@item.StateName</option>
                                                }
                                            }
                                        </select>
                                        <div id="Msg_DriversLicenseState" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divDriversLicenseCopy">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Drivers License Copy <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="file" id="DriversLicenseCopy" name="DriversLicenseCopy" class="form-control" accept=".pdf,.jpeg,.png,.jpg" onchange="RemoveValidation(this);" required />
                                        <label id="lblDriversLicenseCopy"></label>
                                        <div id="Msg_DriversLicenseCopy" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divStateOfEncorp">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>State Of Incorporation <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="StateOfEncorp" name="StateOfEncorp" placeholder="State Of Incorporation" class="form-control" onchange="RemoveValidation(this);" required />
                                        <div id="Msg_StateOfEncorp" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divStateOfIncorporationProof">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>State Of Incorporation Proof <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="file" id="StateOfIncorporationProof" name="StateOfIncorporationProof" class="form-control" placeholder="State Of Incorporation Proof" accept=".pdf,.jpeg,.png,.jpg" onchange="RemoveValidation(this);" required>
                                        <label id="lblStateOfIncorporationProof"></label>
                                        <div id="Msg_StateOfIncorporationProof" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divDateOfEncorp">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label>Date Of Incorporation <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="DateOfEncorp" name="DateOfEncorp" class="form-control" placeholder="Date Of Incorporation" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_DateOfEncorp" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divFedTaxIdentityNo">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="FedTaxIdentityNo">Fed Tax Identity No <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="FedTaxIdentityNo" name="FedTaxIdentityNo" class="form-control" placeholder="Fed Tax Identity No" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_FedTaxIdentityNo" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" id="divFedTaxIdentityProof">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="FedTaxIdentityNo">Fed Tax Identity Proof <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="file" id="FedTaxIdentityProof" name="FedTaxIdentityProof" class="form-control" accept=".pdf,.jpeg,.png,.jpg" placeholder="Fed Tax Identity Proof" onchange="RemoveValidation(this);" required />
                                        <label id="lblFedTaxIdentityProof"></label>
                                        <div id="Msg_FedTaxIdentityProof" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                    </div>
                                    <div class="col-md-4">
                                        <a class="btn btn-secondary pull-right" id="btnnext_1">Next</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!--Tab 2(Address Tab)-->
                <div id="Address" class="tab-pane fade">

                    <div class="container" style="border:1px solid #f8f5f5;width:auto;">

                        <div class="panel-heading">
                            <h6>Primary Business Address</h6>
                            <hr />
                        </div>

                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessAdd1">Line1 <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessAdd1" placeholder="Line1" id="PrimaryBusinessAdd1" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_PrimaryBusinessAdd1" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessAdd1">Line2</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessAdd2" placeholder="Line2" id="PrimaryBusinessAdd2">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessCity">City <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessCity" placeholder="City" id="PrimaryBusinessCity" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_PrimaryBusinessCity" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessCity">State <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessState" placeholder="State" id="PrimaryBusinessState" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_PrimaryBusinessState" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessCity">ZipCode <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessZip" placeholder="ZipCode" id="PrimaryBusinessZip" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_PrimaryBusinessZip" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessPhone">Phone <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="PrimaryBusinessPhone" placeholder="Phone" id="PrimaryBusinessPhone" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_PrimaryBusinessPhone" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessPhone">Email <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="email" class="form-control" name="PrimaryBusinessEmail" placeholder="Email" id="PrimaryBusinessEmail" required>
                                        <div id="Msg_PrimaryBusinessEmail" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div class="container" style="border:1px solid #f8f5f5;width:auto;">
                        <div class="panel-heading">
                            <h6>Billing Address</h6>
                            <hr />
                        </div>

                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="checkbox">
                                    <label><input type="checkbox" id="chk_primary" onchange="check()" />Same as Primary Business Address</label>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessAdd1">Line1 <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingAdd1" placeholder="Line1" id="BillingAdd1" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_BillingAdd1" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="PrimaryBusinessAdd1">Line2</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingAdd2" placeholder="Line3" id="BillingAdd2">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="BillingCity">City <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingCity" placeholder="City" id="BillingCity" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_BillingCity" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="BillingState">State <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingState" placeholder="State" id="BillingState" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_BillingState" class="text-danger">
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="BillingZip">ZipCode <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingZip" placeholder="ZipCode" id="BillingZip" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_BillingZip" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="BillingPhone">Phone <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" name="BillingPhone" placeholder="Phone" id="BillingPhone" onchange="RemoveValidation(this);" required>
                                        <div id="BillingPhone" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <label for="BillingEmail">Email <span style="color:red">*</span></label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="email" class="form-control" name="BillingEmail" placeholder="Email" id="BillingEmail" onchange="RemoveValidation(this);" required>
                                        <div id="Msg_BillingEmail" class="text-danger">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="pull-right" style="padding:20px;">
                        <a id="btnnext_2" class="btn btn-secondary">Next</a>
                    </div>
                   
                </div>
                <div id="Billing" class="tab-pane fade">
                    <div class="panel-heading">
                        <h6>Billing Information</h6>
                        <hr />
                    </div>
                   
                    <div class="container">
                        <div class="col-lg-12">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>Pricing Plan <span style="color:red">*</span></label>
                                            </div>

                                            <div class="col-sm-7">
                                                <select id="PricingPlan" name="PricingPlan" class="form-control" onchange="RemoveValidation(this);" required>
                                                    <option value="0"> Select Option</option>
                                                    @if (ViewBag.pricing != null)
                                                    {
                                                        foreach (var item in ViewBag.pricing)
                                                        {
                                                            <option value="@item.PricingId">@item.PricingType</option>
                                                        }
                                                    }
                                                </select>
                                                <div id="Msg_PricingPlan" class="text-danger">
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>Billing Type <span style="color:red">*</span></label>
                                            </div>
                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" name="BillingType" placeholder="Billing Type" maxlength="50" id="txtBillingType" onchange="RemoveValidation(this);" required>
                                                <div id="Msg_txtBillingType" class="text-danger">
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>Card Type <span style="color:red">*</span></label>
                                            </div>
                                            @{
                                                List<string> cardTypes = new List<string>();
                                                cardTypes.Add("VISA");
                                                cardTypes.Add("Capital One");
                                                cardTypes.Add("Mastercard");
                                            }
                                            <div class="col-sm-7">
                                                <select id="CardType" name="CardType" class="form-control" onchange="RemoveValidation(this);" required>
                                                    <option value="0"> Select Option</option>
                                                    @foreach (var item in cardTypes)
                                                    {
                                                        <option value="@item">@item</option>
                                                    }
                                                </select>
                                                <div id="Msg_CardType" class="text-danger">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>
                                                    Card Number
                                                    <span style="color:red">*</span>
                                                </label>
                                            </div>
                                            <div class="col-sm-7">
                                                <input type="text" class="form-control" maxlength="20" name="CardNumber" placeholder="CardNumber" id="txtCardNumber" onchange="RemoveValidation(this);" required>
                                                <div id="Msg_txtCardNumber" class="text-danger">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>Expiry <span style="color:red">*</span></label>
                                            </div>
                                            @{
                                                string[] Months = { "01", "02", "03", "04", "05", "06", "07", "08", "09", "10", "11", "12" };
                                                string[] Year = { "2019", "2020", "2021", "2022", "2023", "2024", "2025", "2026", "2027", "2028", "2029" };
                                            }
                                            <div class="col-sm-3">
                                                <select class="form-control" name="Month" id="txtExpiryMonth" onchange="RemoveValidation(this);" required>
                                                    <option value="0">Month</option>
                                                    @foreach (var item in Months)
                                                    {
                                                        <option value="@item">@item</option>
                                                    }
                                                </select>
                                                <div id="Msg_txtExpiryMonth" class="text-danger">
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <select class="form-control" name="ExpiryDate" id="txtExpiryYear" onchange="RemoveValidation(this);" required>
                                                    <option value="0">Year</option>
                                                    @if (ViewBag.years != null)
                                                    {

                                                        foreach (var item in ViewBag.years)
                                                        {
                                                            <option value="@item">@item</option>
                                                        }
                                                    }
                                                </select>
                                                <div id="Msg_txtExpiryYear" class="text-danger">
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <label>CVV <span style="color:red">*</span></label>
